{
    "version": "2.0",
    "extensions": {
        "eventHubs": {
            "batchCheckpointFrequency": ${CHECKPOINT},
            "eventProcessorOptions": {
                "maxBatchSize": ${BATCH},
                "prefetchCount": ${PREFETCH}
            }
        }
    },
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
              "isEnabled": true,
              "maxTelemetryItemsPerSecond" : 500
            }
        }
    }
}  